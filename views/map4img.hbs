<!DOCTYPE html>
<html>

<head>
<meta charset='utf-8' />
<meta name='viewport' content='width=device-width, initial-scale=1'>
<title></title>
<script src='./mapbox-gl.js'></script>
<link href='./mapbox-gl_r.css' rel='stylesheet' />

<style>
body { margin: 0; top: 0; bottom: 0; width: 100%; }
#map {
  position: absolute;
  top: 0;
  bottom: 0;
  width: 100%;
}
</style>
</head>

<body>
<div id='map'"></div>

<script>
var map = new mapboxgl.Map({
  container: 'map', 
  attributionControl: false, 
//  style: './style-test.json',
  style: './style-test-vtile2.json',
  hash: true,
  interactive: false,
  antialias: true,
  fadeDuration: 0
})

map.on('load', function () {
})

</script>
</body>
</html>